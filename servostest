import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BOARD)

try:
    GPIO.setup(7, GPIO.OUT)  # Test physical pin 7 (GPIO 4)
    GPIO.setup(33, GPIO.OUT)  # Test physical pin 33 (GPIO 13)
    print("Pins are free to use!")
except RuntimeError as e:
    print(f"Error: {e}")
finally:
    GPIO.cleanup()
